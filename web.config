<?xml version="1.0"?>
<configuration>
  <system.webServer>
    <security>
      <requestFiltering removeServerHeader="true"></requestFiltering>
    </security>
    <httpProtocol>
        <customHeaders>
          <remove name="X-Powered-By"/>
          <clear/>
        </customHeaders>
    </httpProtocol>
  </system.webServer>
</configuration>